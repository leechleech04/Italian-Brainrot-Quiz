{
  "expo": {
    "name": "italian-brainrot-quiz",
    "slug": "italian-brainrot-quiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SahurQuiz.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lch2023.italianbrainrotquiz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lch2023.italianbrainrotquiz"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/PoetsenOne-Regular.ttf"
          ]
        }
      ],
      "expo-asset"
    ]
  }
}
